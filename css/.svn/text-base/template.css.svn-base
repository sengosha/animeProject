@charset "utf-8";

h2.titleTemplate {
	width:100%;
	height:30px;
	line-height:30px;
	font-size:16px;
	background-color:#039;
	color:#FFF;
	text-align:center;
	margin-bottom:10px;
}

.bgTemplate {
	background:url(../img/bg/bgWood.jpg) repeat-y;
	background-size:100%;
}

.div.boxWrapAreaGray {
	margin-bottom:0px;
}

.titleBoxGray {
	text-align:center;
}

div.marginTemplate div.wrapAreaYellow div.areaYellow,
div.marginTemplate div.wrapAreaGray div.areaGray {
	margin-bottom:0;
}

h3.titleTemplate2 {
	width:100%;
	height:25px;
	line-height:25px;
	font-size:12px;
	background-color:#69C;
	color:#FFF;
	text-align:center;
	margin:10px 0 5px;
}

#template {
	margin: 0 auto;
	padding-bottom:15px;
	width: 320px;
	position:relative;
	background:#6CF;
}

#template div.txt-center p {
	margin-bottom:15px;
}


