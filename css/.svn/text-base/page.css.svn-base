/* キャラクタースライドインアニメ */
.MoveChara {
	-webkit-animation: 'MoveChara' 0.5s ease-out 0s 1; 
	-webkit-animation-fill-mode: forwards;
	 position:relative;
}

@-webkit-keyframes MoveChara {
  0%{
    left:-200px;
    }
  100% {
    left:0px;
  }
}

/* キャラクター名前フェイドインアニメ */
#WrapAreaName {
	-webkit-animation: 'WrapAreaName' 0.5s ease-out 0.5s 1; 
	-webkit-animation-fill-mode: forwards;
    opacity:0;
	z-index:100;
}

@-webkit-keyframes WrapAreaName {
  0%{
    opacity:0;
    }
  100% {
    opacity:1;
  }
}

/* トレーニング時上向き矢印 */
.MoveArrowUp {
	-webkit-animation: 'MoveArrowUp' 0.3s ease-in 0s infinite; 
	z-index:100;
	position:relative;
}

@-webkit-keyframes MoveArrowUp {
  0%{
   top:-10px;
    }
  100% {
   top:13px;
  }
}
