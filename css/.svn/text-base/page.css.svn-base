/* インフォメーション */



/* 挨拶履歴 */
#AreaGreetHistory > ul > li {
	padding:5px 5px 10px;
}

#AreaGreetHistory > ul > li:last-child {
	padding:0;
}

#AreaGreetHistory > ul > li > table > tbody > tr:first-child > td:last-child {
	vertical-align:top;
}

#AreaGreetHistory > ul > li > table > tbody > tr > td {
	padding:5px;
	text-align:left;
	vertical-align:bottom;
}

#AreaGreetHistory > ul > li > table > tbody > tr > th {
	vertical-align:top;
	padding:5px;
	width:50px;
}


/* 試合結果 */
#AreaResult > ul > li > a > table > tbody > tr > td {
	width:200px ;
	padding:0 0 0 14px;
	text-decoration:none;
	vertical-align:bottom;
}

#AreaResult > ul > li > a > table > tbody > tr > td:last-child {
	width:60px;
	padding:0 10px 0 5px;
	text-align:right;
	vertical-align:top;
}


