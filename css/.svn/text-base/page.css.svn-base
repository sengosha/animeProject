/* 13.アニメーション
-----------------------------------------------------------------------------*/
/* 動く矢印 クエスト時*/
#MoveArrowQuest {
	position:absolute;
	top:300px;
	right:46px;
	-webkit-animation: 'MoveArrowQuest' 0.4s linear infinite; 
	-webkit-animation-timing-function: ease-in;
	z-index:100;
}


@-webkit-keyframes MoveArrowQuest {
  0% { top:300px;}
  100% { top:315px;}
}

/* 動く矢印 キャプテン任命 */
#MoveArrowCap {
	position:absolute;
	top:680px;
	left:78px;
	-webkit-animation: 'MoveArrowCap' 0.4s ease-in infinite; 
	z-index:100;
}

@-webkit-keyframes MoveArrowCap {
  0% { top:680px;}
  100% { top:695px;}
}

/* 動く矢印 試合開始 */
#MoveArrowMatchStart {
	position:absolute;
	right:40px;
	-webkit-animation: 'MoveArrowMatchStart' 0.4s ease-in infinite; 
	z-index:100;
}

@-webkit-keyframes MoveArrowMatchStart {
  0% { top:360px;}
  100% { top:375px;}
}

/* 試合結果、契約成立テキストアニメ */
.MoveState {
	-webkit-animation: 'MoveState' 1.1s ease-out 1; 
	-webkit-animation-fill-mode: forwards;
	z-index:100;
	margin-top:10px;
}


@-webkit-keyframes MoveState {
  0%{
    -webkit-transform: rotate(-10deg);
    bottom: 0;
    }
  50% {
    -webkit-transform: rotate(-10deg);
  }
  60% {
    -webkit-transform: rotate(20deg);
  }
  70% {
    -webkit-transform: rotate(-11deg);
  }
  80% {
    -webkit-transform: rotate(3deg);
  }
  90% {
    -webkit-transform: rotate(-14deg);
  }
  100% {
    -webkit-transform: rotate(-10deg);
  }
}

