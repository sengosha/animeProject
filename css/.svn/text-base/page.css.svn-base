.Triangle::after {
    content: "";
    position: absolute;
    left: 50px;
    border-width: 10px 10px 0;
    border-style: solid;
    border-color: #666 transparent;
    display: block;
    width: 0;
}

.TriTop::after {
top: -10px;
right: 50px;
bottom: auto;
left: auto;
border-width: 0 10px 10px;
    border-color: #666 transparent;
}

