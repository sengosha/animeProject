	margin-right:1px;
}

.AreaGrayLInner {
	border-top-left-radius:5px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(158,158,158,1)), color-stop(100%,rgba(53,53,53,1))); 
	padding:2px 0 5px 5px;
}


.AreaGrayR {
	position:relative;
	border-top-right-radius:5px;
	border-bottom-right-radius:5px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(69,72,77,1)), color-stop(100%,rgba(0,0,0,1)));
	border:1px solid #000;
	float:left;
	width:143px;
	margin-left:1px;
}

.AreaGrayRInner {
	border-top-right-radius:5px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(158,158,158,1)), color-stop(100%,rgba(53,53,53,1))); 
	padding:2px 5px 5px 0;
}


/*  試合結果のボックススコア　*/
.AreaQr table tbody tr:first-child td{
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(201,4,7,1)), color-stop(100%,rgba(109,0,25,1))); 
}

.AreaQr table tbody tr:first-child td:first-child{
	border-top-left-radius:5px;
}

.AreaQr table tbody tr:first-child td:last-child{
