/* 4.title,shadow
-----------------------------------------------------------------------------*/
.titleRed1 /* 背景をCSSのみにした場合のサンプル */{
	height:33px;
	line-height:33px;
	font-size:16px;
	font-weight:bold;
	text-align:center;
	width:100%;
	background:url(../img/common/lineGray1.jpg),url(../img/common/lineGray1.jpg), -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(186,154,154,0.8)), color-stop(50%,rgba(165,16,16,0.8)), color-stop(53%,rgba(104,0,0,0.8)), color-stop(97%,rgba(183,0,0,0.8)));
	background-repeat:no-repeat, no-repeat;
	background-position:top, bottom;
	background-size:100%;
	text-shadow:
		2px 2px 0px rgba(0,0,0,0.5);
	-webkit-box-shadow:
		0px 0px 6px rgba(000,000,000,0.8),
		inset 0px 0px 3px rgba(255,255,255,0.1);
}

/* 赤帯 上下のボーダーあり*/
.titleRed2 {
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(173,3,6,0.8)), color-stop(100%,rgba(76,0,1,1))); 
	border-top:#24211C 1px solid;
	border-bottom:#24211C 1px solid;
	text-align:left;
}

/* 黒帯 バーグラデーション*/
.titleBlack1 {
background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,0.8)), color-stop(50%,rgba(153,153,153,0.8)), color-stop(100%,rgba(0,0,0,0.8))); 
	width:100%;
}

/* ボックスシャドウ　上部に白い影をつける */
.shadowWhite1 {
	-webkit-box-shadow:
		0px 0px 0px rgba(000,000,000,0),
		inset 0px 3px 5px rgba(255,255,255,1);
}


/* ボックスシャドウ　上部に黒い影をつける */
.shadowBlack2 {
	-webkit-box-shadow:
		0px 0px 0px rgba(000,000,000,0),
		inset 0px 3px 4px rgba(0,0,0,0.9);
}


/* 5.gradient,bgcolor
-----------------------------------------------------------------------------*/
/* グラデーション赤1 // 上から下に向かって赤*/
.gradeRed1 {
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(155,3,3,1)), color-stop(100%,rgba(86,0,0,1)));
}

/* グラデーション青1 // 上から下に向かって青*/
.gradeBlue1 {
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(44,83,158,1)), color-stop(100%,rgba(17,34,61,1)));
}

/* グラデーション黒1 // 上から下に向かってグレー → 黒*/
.gradeBlack1 {
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(102,102,102,0.8)), color-stop(100%,rgba(0,0,0,0.8))); 
}

/* グラデーション黒2　// 左から右に向かって透過 */
.gradeBlack2 {
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,1)), color-stop(100%,rgba(0,0,0,0.5)));
}

/* グラデーション黒3 // 上から下に向かって位置80　黒100％　→ 黒0％*/
.gradeBlack3 {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,1)), color-stop(80%,rgba(0,0,0,1)), color-stop(97%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0))); 
}

/* 20%白　単色背景 */
.bgWhite1 {
	background-color:rgba(255,255,255,0.2);
}

/* 黒　単色背景 */
.bgBlack1 {
	background-color:rgba(0,0,0,1);
}



