/* ３個並びタブボタン */
.btnTab li {
	border: 1px solid #AAA;
	display: inline-block;
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(130,140,149,1)), color-stop(100%,rgba(0,0,0,1))); 
	box-shadow: 0 3px 3px rgba(0, 0, 0, 0.4), inset 0 1px 0 #FFF;
}

.btnTab li.tabSelected {
	background: -webkit-linear-gradient(top, #ECECEC 50%, #D1D1D1 100%);
}

.btnTab li a{
	padding: 2px 20px;
	display:block;
	line-height:30px;
	color:#FFF;
}

.btnTab li.tabSelected a{
	color:#000;
}

/* 4.各種ゲージ
-----------------------------------------------------------------------------*/
/* 各ゲージ外枠  */

.wrapGage /* 汎用 */{
	width: 100%;
	height:7px;
	border-radius:3px;
	padding:1px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(40,52,59,1)), color-stop(76%,rgba(130,140,149,1)), color-stop(100%,rgba(181,189,200,1)));	
	border:#999999 1px solid;
}

.wrapGageQt /* クエスト用 */{
	width: 100%;
	height:5px;
	border-radius:3px;
	padding:1px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(40,52,59,1)), color-stop(76%,rgba(130,140,149,1)), color-stop(100%,rgba(181,189,200,1)));	
	border:#999999 1px solid;
}

/* 経験値ゲージ */
.innerGageExp {
	display:block;
	height: 100%;
	border-radius:2px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(51%,rgba(198,182,137,1)), color-stop(100%,rgba(199,152,16,1))); 
}

/* 経営Ptゲージ  */
.innerGageAct {
	display:block;
	height: 100%;
	border-radius:2px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,154,142,1)), color-stop(100%,rgba(255,26,0,1)));
}

/* サラリーゲージ  */
.innerGageSly {
	display:block;
	height:100%;
	border-radius:2px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(73,155,234,1)), color-stop(100%,rgba(0,4,255,1))); 
}

/* 達成率ゲージ  */
.innerGageQuest {
	display:block;
	height:100%;
	border-radius:2px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(210,255,82,1)),color-stop(100%,rgba(145,232,66,1)));
}


