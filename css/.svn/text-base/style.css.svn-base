/* リストで並ぶインフォメーションエリアのベース　スタイルが少し違う場合は、ページごとに調整。  */
.areaInfo {
	margin:10px 0;
}

.areaInfo > ul > li {
	position:relative;
	background:url(../img/common/lineGray1.jpg), -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(69,72,77,0.85)), color-stop(100%,rgba(0,0,0,0.85)));
	background-repeat:no-repeat;
	background-position:bottom;
	background-size:100%;
}

.areaInfo > ul > li > a {
	display:block;
	width:100%;
	padding:5px 0 10px;
	text-align:left;
}

.areaInfo > ul > li > a.areaReadMore {
	background:url(../img/common/lineGray1.jpg), -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(153,153,153,0.85)), color-stop(100%,rgba(0,0,0,0.85)));
	background-repeat:no-repeat;
	background-position:bottom;
	background-size:100%;
	padding:0;
	width:100%;
	height:35px;
	text-align:center;
	line-height:35px;
	text-decoration:none;
	color:#FFF;
	text-shadow:
		-1px -1px 0px rgba(000,000,000,1),
		0px 0px 0px rgba(0,0,0,1);
	font-size:16px;
}

.areaInfo > ul > li > a > table > tbody > tr > th {
	width:40px;
	text-indent:1em;
	vertical-align:top;
}

.areaInfo > ul > li > a > table > tbody > tr > td {
	width:230px;
	padding:3px 50px 0 14px;
	vertical-align:bottom;
}

.areaInfo > ul > li span.arrow {
	background:url(../img/btn/arrow.png) no-repeat center top;
	background-size: 100% auto;
	width:21px;
	height:14px;
	position:absolute;
	top:32%;
	right:10px;
}

