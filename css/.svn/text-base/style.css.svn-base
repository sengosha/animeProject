/* 2.container
-----------------------------------------------------------------------------*/
/* 黒色背景 */
#container {
	margin: 0 auto;
	width: 320px;
	min-height:390px;
	overflow:hidden;
	position:relative;
	z-index:0;
	padding-bottom:20px;
}

/* 体育館床背景  */
#containInner {
	background:url(../img/bg/bgWood.jpg) repeat-y;
	background-size:100%;
}

/* 背景調整用  */
#containInnerSt {
	/*background:url(../img/bg/bgSt.jpg) repeat-y;ストリート背景
	background-size:100%;*/
	background-color:#000;
	padding-bottom:20px;
}

/* 背景サイズ調整用  */
.bgAdjust {
	min-height:155px;
}


/* 3.header
-----------------------------------------------------------------------------*/
#header {
	position:relative;
}

#header h1 {
	width:100%;
	height:25px;
	background:url(../img/dummy/headerLogo.jpg) no-repeat center top;
	background-size:100%;
	text-indent:-9999em;
	overflow:hidden;
}

#header p#btnBack{
	position:absolute;
	top:2px;
	left:10px;
}

#header p#btnReload{
	position:absolute;
	top:2px;
	right:10px;
}


/* 4.footer
-----------------------------------------------------------------------------*/
#WrapFooter {
	width:320px;
	height:34px;
	margin:0 auto;
}

#footer {
	width:320px;
	height:45px;
}

#footer nav ul li{
	float: left;
}

/* 5.popup menu
-----------------------------------------------------------------------------*/

#bgLayer, #popupMenu, #btnClose{
	position: absolute;
	display: none;
}

body > #bgLayer{ 
    height:100%; 
}

#bgLayer {
	width: 100%;
	height:100%;
	background-color: #000;
	opacity: 0.6;
	z-index: 20;
}

#btnClose {
	display: block;
	color: #666;
	top: -10px;
	right: -10px;
}

#popupMenu {
	z-index: 30;
	color: #000;
	width:294px;
	padding:10px 20px;
	text-align:center;
	padding:10px 0;
	border:2px #999 solid;
	border-radius:5px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(69,72,77,0.6)), color-stop(100%,rgba(0,0,0,0.6)));
		-webkit-box-shadow:
		0px 0px 2px rgba(0,0,0,0.5),
		inset 0px 0px 2px rgba(0,0,0,0.5);
}

#popupMenu p{
	color:#FFF;
	font-size:20px;
}
#popupMenu > ul {
	margin:15px 13px 15px 19px;
}

#popupMenu > ul > li > a{
	font-size: 16px;
	color: #000000;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(254,255,252,1)), color-stop(47%,rgba(182,141,76,1)), color-stop(100%,rgba(255,210,99,1))); 
		border-radius: 5px;
	-webkit-border-radius: 5px;
	border: 1px solid #e0e0e0;
	-webkit-box-shadow:
		0px 1px 3px rgba(000,000,000,0.5),
		inset 0px 0px 3px rgba(255,255,255,0.1);
	text-shadow:
		1px 1px 0px rgba(255,255,255,1);
	float:left;
	width:80px;
	text-decoration:none;
	margin:0 5px 5px 0;
	padding:13px 0;
}

